<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Новости</title>
  </head>
  <body>
    <% include blocks/header.ejs %>
    <h1>Новости</h1>
    <ul>
      <a href="/addNews">Добавить новсть</a>
      <% data.forEach(function(item) { %>
        <li>
          <a href="/news/<%= item._id %>"><h2><%= item.title %></h2></a>
          <p><%= item.text %></p>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
